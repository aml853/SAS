/*NEXT DATA SET*/



/*fortune_credit*/

/*PROC CONTENTS for viewing summary-level dataset contents */ 
proc contents data= fortune_credit; run; 

/*PROC PRINT for viewing row-level dataset contents. */ 
proc print data=fortune_credit (firstobs = 1 obs =10); run; 



